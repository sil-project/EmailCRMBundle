{
    "name": "libre-informatique/email-crm-bundle",
    "type": "symfony-bundle",
    "description": "CRM Bundle for Symfony with Email management",
    "require": {
	"php": ">=5.6",
	"symfony/symfony": ">=3.2",
	"blast-project/core-bundle" : "dev-wip-lisem",
        "blast-project/base-entities-bundle"  : "dev-wip-lisem",
	"blast-project/outer-extension-bundle": "dev-wip-lisem",
        "blast-project/utils-bundle": "dev-wip-lisem",
        "libre-informatique/media-bundle": "dev-wip-lisem",
        "libre-informatique/email-bundle": "dev-wip-lisem",
        "libre-informatique/crm-bundle": "dev-wip-lisem"
    },
    "require-dev": {
	"symfony/phpunit-bridge": ">=3.2"
    },
    "license": "LGPL-3.0",
    "keywords": [
        "e-venement",
        "libio",
        "sonata",
        "symfony",
        "CRM",
        "Email"
    ],
    "homepage": "https://github.com/libre-informatique/SymfonyLibrinfoEmailCRMBundle",
    "authors": [
        {
            "name": "Marcos Bezerra de Menezes",
            "email": "marcos.bezerra@libre-informatique.fr"
        },
        {
            "name": "Libre Informatique",
            "homepage": "http://www.libre-informatique.fr/"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev",
            "dev-wip-lisem": "0.4.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Librinfo\\EmailCRMBundle\\": "src/"
        }
    }
}
